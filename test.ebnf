program = decl, {decl};

decl =;


int = 
   [-]
   ,
   "0" 
   | "1-9",{DIGIT}
;

float =
   int, '.' , DIGIT, {DIGIT}
;

currency = 
   (
      int 
      | float
   )
   ,
   CURRENCY_NAME
;

string = "'", {CHAR} "'";

DIGIT = "0-9";
LETTER =
   "a-z"
   | "A-Z"
;
SYMBOL = "[" | "]" | "{" | "}" | "(" | ")" | "<" | ">" | "'" | '"' | "=" | "|" | "." | "," | ";" "_";

CHAR = 
   DIGIT
   | LETTER
   | SYMBOL
;

CURRENCY_NAME = FROM FILE; // I.E. 'PLN' | 'EUR' | 'USD'; 